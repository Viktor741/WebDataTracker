<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="401.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox layoutX="-1.0" prefHeight="401.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="401.0" prefWidth="600.0" VBox.vgrow="ALWAYS">
               <children>
                  <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button fx:id="GetPageButton" mnemonicParsing="false" onMouseClicked="#getValue" text="GetPage">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                        <TextField fx:id="pageField" promptText="copy web adress here" />
                        <TextField fx:id="ObjectField" promptText="Write here name of thing that you want to track" />
                           <Label fx:id="valueLabel">
                              <font>
                                 <Font size="20.0" />
                              </font>
                           </Label>
                     </children>
                  </VBox>
                  <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                     <children>
                        <Button fx:id="LaunchTrackerButton" mnemonicParsing="false" onMouseClicked="#LaunchTracker" text="LaunchTracker">
                           <font>
                              <Font size="20.0" />
                           </font>
                        </Button>
                        <TextField fx:id="trackingValue" promptText="Write here tracking interval in seconds" />
                           <TextField fx:id="limit" promptText="Add here for how long do you want to track in secons" />
                           <Label fx:id="trackerLabel">
                              <font>
                                 <Font size="20.0" />
                              </font>
                           </Label>
                     </children>
                  </VBox>
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
